{
  "name": "meiko",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@8.5.0",
  "main": "dist/meiko.common.js",
  "module": "dist/meiko.esm.js",
  "files": [
    "src/",
    "dist/*.js"
  ],
  "exports": {
    ".": {
      "import": {
        "node": "./dist/meiko.common.js",
        "default": "./dist/meiko.esm.js"
      },
      "require": "./dist/meiko.common.js",
      "types": "./types/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./types/*": "./types/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "rollup -c ./build/dev.js",
    "build:prod": "rollup -c ./build/prod.js",
    "build-dts": "tsc -p tsconfig.build.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --ext .js,.ts",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "meiko",
    "reactive",
    "js-compiler",
    "js-library",
    "js-framework"
  ],
  "author": "AYIN",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@types/quick-hash": "^1.0.2",
    "@typescript-eslint/parser": "^6.11.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.1.0",
    "mini-css-extract-plugin": "^2.8.0",
    "picocolors": "^1.0.0",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^3.1.0",
    "quick-hash": "^1.0.4",
    "rollup": "^4.5.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "terser": "^5.27.0",
    "terser-webpack-plugin": "^5.3.10",
    "thread-loader": "^4.0.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "webpack": "^5.90.3",
    "webpack-merge": "^5.10.0"
  },
  "devDependencies": {
    "image-minimizer-webpack-plugin": "^4.0.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1"
  }
}
