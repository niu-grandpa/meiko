{
  "type": "module",
  "name": "meiko",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@8.5.0",
  "main": "dist/meiko.cjs",
  "module": "dist/meiko.mjs",
  "unpkg": "dist/meiko.umd.js",
  "jsdelivr": "dist/meiko.umd.js",
  "files": [
    "src/",
    "dist/*.js"
  ],
  "exports": {
    ".": {
      "import": {
        "node": "./dist/meiko.cjs",
        "default": "./dist/meiko.mjs"
      },
      "require": "./dist/meiko.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./types/*": "./types/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "build-dts": "tsc -p tsconfig.build.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --ext .js,.ts",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "mko",
    "meko",
    "meiko",
    "reactive",
    "js-compiler",
    "js-library",
    "js-framework"
  ],
  "author": "AYIN",
  "license": "MIT",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rollup": "^4.5.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
